{
  "files.exclude": {
    "**/*.pyc": { "when": "$(basename).py" },
    "**/__pycache__": true
  },
  "python.analysis.typeCheckingMode": "strict",
  "python.analysis.packageIndexDepths": [
    {
      "name": "sqlalchemy",
      "depth": 2
    },
    {
      "name": "fastapi",
      "depth": 2
    }
  ]
}
